         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/elements/1.1/">

    <!-- Declaration of Classes based on the taxonomic classification -->
    
    <!-- Kingdom Animalia -->
    <owl:Class rdf:about="https://www.gbif.org/species/1">
        <rdfs:label xml:lang="la">Animalia</rdfs:label>
    </owl:Class>

    <!-- Phylum Arthropoda -->
    <owl:Class rdf:about="https://www.gbif.org/species/54">
        <rdfs:label xml:lang="la">Arthropoda</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://www.gbif.org/species/1"/>
    </owl:Class>

    <!-- Class Insecta -->
    <owl:Class rdf:about="https://www.gbif.org/species/216">
        <rdfs:label xml:lang="la">Insecta</rdfs:label>
