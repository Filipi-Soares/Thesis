         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/elements/1.1/">

    <!-- Declaration of Classes based on the taxonomic classification -->
    
    <!-- Kingdom Animalia -->
    <owl:Class rdf:about="https://www.gbif.org/species/1">
        <rdfs:label xml:lang="lat">Animalia</rdfs:label>
    </owl:Class>

    <!-- Phylum Chordata -->
