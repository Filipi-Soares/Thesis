        <rdfs:label xml:lang="lat">Chordata</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://www.gbif.org/species/1"/>
    </owl:Class>

    <!-- Class Mammalia -->
    <owl:Class rdf:about="https://www.gbif.org/species/359">
        <rdfs:label xml:lang="lat">Mammalia</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://www.gbif.org/species/44"/>
    </owl:Class>

    <!-- Order Artiodactyla -->
