    <owl:Class rdf:about="https://www.gbif.org/species/6">
        <rdfs:label xml:lang="lat">Plantae</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="https://www.gbif.org/species/7707728">
        <rdfs:label xml:lang="lat">Tracheophyta</rdfs:label>
        <rdfs:subClassOf rdf:resource="https://www.gbif.org/species/6"/>
    </owl:Class>

</rdf:RDF>
