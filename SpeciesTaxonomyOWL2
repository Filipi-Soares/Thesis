<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#">

    <!-- Namespaces and ontology declaration would be here -->

    <!-- Malus domestica -->
    <owl:Class rdf:about="http://www.gbif.org/species/6">
        <rdfs:label xml:lang="lat">Plantae</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="http://www.gbif.org/species/7707728">
        <rdfs:label xml:lang="lat">Tracheophyta</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.gbif.org/species/6"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.gbif.org/species/220">
        <rdfs:label xml:lang="lat">Magnoliopsida</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.gbif.org/species/7707728"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.gbif.org/species/691">
